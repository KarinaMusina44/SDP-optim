        "# Fonctions utilitaires (réutilisées en Q2/Q3)\n",
        "def calc_contributions(notes: dict, poids: dict, x: str, y: str, criteres=None):\n",
        "    \"\"\"Calcule contrib(k)=w_k*(x_k - y_k) pour tous les critères k.\"\"\"\n",
        "    if criteres is None:\n",
        "        criteres = list(poids.keys())\n",
        "    return {k: poids[k] * (notes[x][k] - notes[y][k]) for k in criteres}\n",
        "\n",
        "def pros_cons_neutral(contrib: dict):\n",
        "    \"\"\"Retourne (pros, cons, neutral) à partir d'un dict de contributions.\"\"\"\n",
        "    pros = [k for k, v in contrib.items() if v > 0]\n",
        "    cons = [k for k, v in contrib.items() if v < 0]\n",
        "    neutral = [k for k, v in contrib.items() if v == 0]\n",
        "    return pros, cons, neutral\n"
      ]
