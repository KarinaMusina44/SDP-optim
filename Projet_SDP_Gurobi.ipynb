      "source": [
        "# Données de l'exemple (x ≻ y)\n",
        "criteres = [\"A\",\"B\",\"C\",\"D\",\"E\",\"F\",\"G\"]\n",
        "\n",
        "notes_x = {\"A\":85,\"B\":81,\"C\":71,\"D\":69,\"E\":75,\"F\":81,\"G\":88}\n",
        "notes_y = {\"A\":81,\"B\":81,\"C\":75,\"D\":63,\"E\":67,\"F\":88,\"G\":95}\n",
        "\n",
        "poids = {\"A\":8,\"B\":7,\"C\":7,\"D\":6,\"E\":6,\"F\":5,\"G\":6}\n",
